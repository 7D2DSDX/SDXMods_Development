<configs>
	<!--  This tells SDX to add to the blocks.xml -->
	<config name="blocks">	
		<!-- This tells SDX to add the following block to the bottom of the block list -->
		<append xpath="/blocks">	
			<block id="" name="Stone01">
				<property name="Extends" value="rock01" />
				<property name="Model" value="#DesertStones?Stone01" />
			</block>
			<block id="" name="Stone02">
				<property name="Extends" value="rock01" />
				<property name="Model" value="#DesertStones?Stone02" />
			</block>
			<block id="" name="Stone03">
				<property name="Extends" value="rock01" />
				<property name="Model" value="#DesertStones?Stone03" />
			</block>
			<block id="" name="Stone04">
				<property name="Extends" value="rock01" />
				<property name="Model" value="#DesertStones?Stone04" />
			</block>
			<block id="" name="Stone05">
				<property name="Extends" value="rock01" />
				<property name="Model" value="#DesertStones?Stone05" />
			</block>
			<block id="" name="Stone06">
				<property name="Extends" value="rock01" />
				<property name="Model" value="#DesertStones?Stone06" />
			</block>
			<block id="" name="Stone07">
				<property name="Extends" value="rock01" />
				<property name="Model" value="#DesertStones?Stone07" />
			</block>
			<block id="" name="Stone08">
				<property name="Extends" value="rock01" />
				<property name="Model" value="#DesertStones?Stone08" />
			</block>




		</append>
	</config>

	<config name="biomes">
		<append xpath="/worldgeneration/biomes/biome[@name='desert']/decorations">	
			<decoration type="block" blockname="Stone01" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="Stone02" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="Stone03" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="Stone04" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="Stone05" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="Stone06" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="Stone07" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="Stone08" prob="0.001" rotatemax="7" />


		</append>


	</config>
</configs>